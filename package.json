  "license": "ISC",
  "dependencies": {
    "axios": "^1.1.3",
    "bcryptjs": "^2.4.3",
    "config": "^3.3.8",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-validator": "^6.14.2",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.7.2",
    "react-router-dom": "^6.4.3",
    "shortid": "^2.2.16"
  },
  "devDependencies": {
    "concurrently": "^7.5.0",
    "cross-env": "^7.0.3",
    "nodemon": "^2.0.20"
  }
}
